<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">
  <% include header %>
  <!-- Left side column. contains the logo and sidebar -->
  <% include leftsidebar %>
  <!-- Content Wrapper. Contains page content -->
  <% include content %>
  <!-- /.content-wrapper -->
  <% include footer %>
  <!-- Control Sidebar -->
  <% include controlsidebar %>
  <!-- /.control-sidebar -->
  <!--modals-->
  <div class="user-Operation-block-modals">
    <% include modal/file-upload-modal %>
    <% include modal/disease-add-modal %>
  </div>
  <!--./modals-->
  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->
  <div class="control-sidebar-bg"></div>
</div>
<!-- ./wrapper -->

<!-- jQuery 2.2.3 -->
<script src="/javascripts/jquery/jquery-2.2.3.min.js"></script>
<!-- Bootstrap 3.3.6 -->
<script src="/javascripts/bootstrap3/bootstrap.min.js"></script>
<!--sweetalert-->
<script src="/javascripts/sweetalert/sweetalert.min.js"></script>
<!-- FastClick -->
<script src="/javascripts/fastclick/fastclick.min.js"></script>
<!-- AdminLTE App -->
<script src="/javascripts/dataplatform/dataplatform.min.js"></script>
<!-- Sparkline -->
<script src="/javascripts/sparkline/jquery.sparkline.min.js"></script>
<!-- jvectormap -->
<script src="/javascripts/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
<script src="/javascripts/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
<!-- SlimScroll 1.3.0 -->
<script src="/javascripts/slimscroll/jquery.slimscroll.min.js"></script>
<!-- ChartJS 1.0.1 -->
