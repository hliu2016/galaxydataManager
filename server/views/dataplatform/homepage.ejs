  <!-- Bootstrap 3.3.6 -->
  <link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap3/bootstrap.min.css">
  <!-- Font Awesome -->
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudfla；re.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <!-- jvectormap -->
  <link rel="stylesheet" type="text/css" href="/stylesheets/jvectormap/jquery-jvectormap-1.2.2.css">
  <!-- Theme style -->
  <link rel="stylesheet" type="text/css" href="/stylesheets/dataplatform/dataplatform.min.css">
  <link rel="stylesheet" type="text/css" href="/stylesheets/dataplatform/dataplatform-modal-111017.css">
  <link rel="stylesheet" type="text/css" href="/stylesheets/dataplatform/dataplatform-custom-ej11894567.css">
  <!-- AdminLTE Skins. Choose a skin from the css/skins
       folder instead of downloading all of them to reduce the load. -->
  <link rel="stylesheet" type="text/css" href="/stylesheets/dataplatform/skins/_all-skins.min.css">
  <!--sweetalert-->
  <link rel="stylesheet" type="text/css" href="/stylesheets/sweetalert/sweetalert.css" />
  <!--w3.css-->
  <link rel="stylesheet" href="/stylesheets/w3/w3.css">
